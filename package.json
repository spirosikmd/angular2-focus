{
  "name": "angular2-focus",
  "version": "0.0.0-development",
  "description": "Angular attribute directive that gives focus on an element depending on a given expression",
  "main": "index.js",
  "module": "index.js",
  "scripts": {
    "test": "karma start",
    "lint": "tslint -c tslint.json 'index.ts src/**/*.ts docs/**/*.ts'",
    "start": "ngc",
    "prepublish": "ngc",
    "docs": "concurrently \"watchify docs/main.ts -p [ tsify ] -o docs/bundle.js\" \"lite-server\"",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "commit": "git-cz",
    "add-contributor": "all-contributors add",
    "generate-contributors": "all-contributors generate"
  },
  "keywords": [
    "angular",
    "angular2",
    "@angular",
    "focus"
  ],
  "author": "Spyros Ioakeimidis <spyrosikmd@gmail.com> (http://www.spyros.io)",
  "license": "MIT",
  "dependencies": {
    "@angular/core": "^4.0.0"
  },
  "devDependencies": {
    "@angular/common": "^2.1.0",
    "@angular/compiler": "^2.1.0",
    "@angular/compiler-cli": "^2.4.9",
    "@angular/forms": "^2.4.10",
    "@angular/platform-browser": "^2.1.0",
    "@angular/platform-browser-dynamic": "^2.1.0",
    "@angular/platform-server": "^2.1.0",
    "@types/core-js": "^0.9.34",
    "@types/jasmine": "^2.5.46",
    "all-contributors-cli": "^4.0.0",
    "awesome-typescript-loader": "^3.0.7",
    "browserify": "^14.1.0",
    "commitizen": "^2.9.6",
    "concurrently": "^3.1.0",
    "core-js": "^2.4.1",
    "cz-conventional-changelog": "^1.2.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.4.1",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.2",
    "lite-server": "^2.2.2",
    "null-loader": "^0.1.1",
    "rxjs": "^5.1.1",
    "semantic-release": "^6.3.2",
    "tsify": "^3.0.1",
    "tslint": "^4.4.2",
    "typescript": "^2.0.3",
    "wallaby-webpack": "^0.0.34",
    "watchify": "^3.7.0",
    "webpack": "^2.2.1",
    "zone.js": "^0.8.0"
  },
  "typings": "./index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/spirosikmd/angular2-focus"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
